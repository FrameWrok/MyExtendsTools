<UserControl xmlns:UserControls="clr-namespace:BackFiles_New.UserControls.ChildControls" 
             x:Class="BackFiles_New.UserControls.WebBrower"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BackFiles_New.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Grid.Column="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>



            <Button Width="100" Name="btn_openurl" Click="btn_openurl_Click" Grid.Column="0">打开url</Button>
            <Button Width="100" Name="btn_regjs" Click="btn_regjs_Click"  Grid.Column="1">注入js</Button>
            <Button Width="100" Name="btn_saveimg" Click="btn_saveimg_Click" Grid.Column="2">保存图片</Button>
        </Grid>
        <WebBrowser Name="web" Grid.Row="1"/>
    </Grid>
</UserControl>
